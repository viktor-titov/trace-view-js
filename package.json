{
  "name": "@viktor-titov/trace-view-js",
  "version": "0.0.6",
  "description": "View Trace with spans.",
  "author": "Viktor Titov",
  "homepage": "https://github.com/viktor-titov/trace-view-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/viktor-titov/trace-view-js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/viktor-titov"
  },
  "main": "dist/trace-view-js.js",
  "module": "dist/trace-view-js.module.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "umd": "./dist/trace-view-js.umd.js",
      "require": "./dist/trace-view-js.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/trace-view-js.module.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "build:types": "tsc --emitDeclarationOnly",
    "build:bundle": "webpack --mode=production --config=./webpack/lib.config.js",
    "build": "npm run clean && npm run build:types && npm run build:bundle",
    "bundle:analyzer": "webpack --mode=production --config=./webpack/lib.analyzer.config.js"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@babel/runtime": "^7.24.6",
    "@types/classnames": "^2.3.1",
    "@types/deep-freeze": "^0.1.5",
    "@types/hoist-non-react-statics": "^3.3.5",
    "@types/lodash": "^4.17.4",
    "@types/moment": "^2.13.0",
    "@types/react": "^17.0.80",
    "@types/react-dom": "^16.0.0",
    "@types/react-icons": "^2.2.7",
    "@types/recompose": "^0.30.15",
    "@types/tinycolor2": "^1.4.6",
    "babel-loader": "^9.1.3",
    "babel-plugin-add-module-exports": "^1.0.4",
    "chance": "^1.1.11",
    "classnames": "^2.5.1",
    "clean-webpack-plugin": "^4.0.0",
    "combokeys": "^3.0.1",
    "copy-to-clipboard": "^3.3.3",
    "deep-freeze": "^0.0.1",
    "emotion": "^10.0.27",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "fuzzy": "^0.1.3",
    "hoist-non-react-statics": "^3.3.2",
    "json-markup": "^1.1.4",
    "lodash": "^4.17.21",
    "lru-memoize": "^1.1.0",
    "memoize-one": "^5.2.1",
    "mini-css-extract-plugin": "^2.9.0",
    "moment": "^2.30.1",
    "react-icons": "^2.2.7",
    "recompose": "^0.25.1",
    "rimraf": "^5.0.7",
    "terser-webpack-plugin": "^5.3.10",
    "tinycolor2": "^1.4.1",
    "tween-functions": "^1.2.0",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  }
